#include<iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter the number of elements : ";
    cin>>n;
    int arr[n];
    int i;
    for(i=0;i<n;i++) {
        cin>>arr[i];
    } 
    int x;
    bool flag = false;
    cout<<"Enter targeted element : ";
    cin>>x;
    int index;
    for(i=0;i<n;i++) {
        if(x==arr[i]) {
            flag = true;
            index = i;
            break;
        }
    }
    if(flag == true) {
        cout<<x <<" is found at index "<<index<<endl;
    }
    else {
        cout<<x<<"is not found"<<endl;
    }
    return 0;
}
