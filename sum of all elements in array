#include<iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter the no of elements in array : "<<endl;
    cin>>n;
    int arr[n];
    int i;
    cout<<"Enter all elements : "<<" ";
    for(i=0;i<n;i++) {
        cin>>arr[i];
    }
    int sum = 0;
    for(i=0;i<n;i++) {
        sum = sum + arr[i];
    }
    cout<<"Sum of all elements is : "<<sum<<endl;
    return 0;
}
