#include<iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter the number of elements : ";
    cin>>n;
    int arr[n];
    int i;
    cout<<"Enter all elements : "<<" ";
    for(i=0;i<n;i++) {
        cin>>arr[i];
    }
    int prod = 1;
    for(i=0;i<n;i++) {
        prod = prod * arr[i];
    }
    cout<<"Product of all elements is : "<<prod<<endl;
    return 0;
}
